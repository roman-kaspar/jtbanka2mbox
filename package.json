{
  "name": "jtbanka2mbox",
  "version": "1.0.0",
  "description": "J&T Banka XML --> sMoneybox CSV",
  "repository": "git@github.com:roman-kaspar/jtbanka2mbox.git",
  "author": "Roman Kaspar <roman.kaspar@seznam.cz>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc --esModuleInterop --target ES2020 --module commonjs --outDir dist src/jtbanka2mbox.ts",
    "dev:build": "tsc --esModuleInterop --watch --target ES2020 --module commonjs --outDir dist src/jtbanka2mbox.ts"
  },
  "renovate": {
    "extends": [
      "config:semverAllMonthly"
    ],
    "bumpVersion": "patch",
    "automerge": true,
    "major": {
      "automerge": false
    }
  },
  "devDependencies": {
    "@types/node": "^14.14.35",
    "typescript": "^4.2.3"
  }
}
